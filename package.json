{
  "name": "@teranes/vue-composables",
  "version": "3.0.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolvers/index.d.ts",
      "import": "./dist/resolvers/index.js",
      "require": "./dist/resolvers/index.cjs"
    }
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "clean": true
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teranes10/vue-composables.git"
  },
  "keywords": [],
  "author": "Teranes",
  "license": "MIT",
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.26",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "description": ""
}

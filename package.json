{
  "name": "@teranes/vue-composables",
  "type": "module",
  "version": "5.0.13",
  "description": "Collection of Vue Composables",
  "author": "Teranes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teranes10/vue-composables.git"
  },
  "keywords": [],
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.js",
      "require": "./dist/resolver.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "release": "turbo run build && changeset && changeset version && changeset publish && git add . && git commit -m \"Automated build commit\" && git push",
    "patch": "git add . && git commit -m \"Automated build commit\" && npm version patch && git push",
    "build": "npm run test && node process.js && npm run lint:fix && tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:dev": "vitest",
    "dev": "vite ./apps/playground"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@changesets/cli": "^2.27.8",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^20.11.26",
    "import-meta-resolve": "^4.1.0",
    "jsdom": "^24.1.0",
    "rollup": "^4.22.4",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-summary": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "terser": "^5.31.3",
    "tsup": "^8.1.0",
    "turbo": "^2.1.2",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  }
}
